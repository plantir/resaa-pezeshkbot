  <table style="width:100%;" cellspacing="0" cellpadding="0">
    <thead>
      <th>ردیف</th>
      <th>کد کالا</th>
      <th>شرح کالا یا خدمات</th>
      <th>تعداد/مقدار</th>
      <th>واحد اندازه گیری</th>
      <th>مبلغ واحد (ریال)</th>
      <th>مبلغ کل(ریال)</th>
      <th>مبلغ تخفیف(ریال)</th>
      <th>مبلغ کل پس از تخفیف(ریال)</th>
      <th>جمع مالیات و عوارض(ریال)</th>
      <th>جمع مبلغ کل بعلاوه جمع مالیات و عوارض(ریال)</th>
    </thead>
    <tbody>
      <tr>
        <td>1</td>
        <td></td>
        <td>{{order.selected_test.name}}</td>
        <td>{{persianDigit(order.count)}}</td>
        <td>تست</td>
        <td>{{persianDigit(currency(order.unit_amount))}} </td>
        <td>{{persianDigit(currency(order.unit_amount * order.count))}} </td>
        <td>{{persianDigit(currency(order.discount_amount ))}} </td>
        <td>{{persianDigit(currency(order.unit_amount * order.count - order.discount_amount))}} </td>
        <td>{{persianDigit(currency(order.tax_amount))}} </td>
        <td>{{persianDigit(currency(order.payable_amount))}} </td>
      </tr>
    </tbody>
    <tfoot>
      <tr>
        <td colspan="6" style="text-align: right;">جمع کل </td>
        <td>{{persianDigit(currency(order.unit_amount * order.count))}} </td>
        <td>{{persianDigit(currency(order.discount_amount ))}} </td>
        <td>{{persianDigit(currency(order.unit_amount * order.count - order.discount_amount))}} </td>
        <td>{{persianDigit(currency(order.tax_amount))}} </td>
        <td>{{persianDigit(currency(order.payable_amount))}} </td>
      </tr>
      <tr>
        <td colspan="6" style="text-align: right;"> شرایط و نحوه فروش : <span style="margin-right:10px">نقدی</span><span
            class="checkbox"></span> <span style="margin-right:10px">غیر نقدی</span><span class="checkbox"></span>
        </td>
        <td colspan="6" style="text-align: right;"> توضیحات: </td>

